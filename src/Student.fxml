<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="500.0" prefWidth="305.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Student">
   <children>
   <Label text="Name">
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </VBox.margin></Label>
    <TextField fx:id="nameField" maxWidth="150">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin></TextField>
    <Label text="Phone">
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </VBox.margin></Label>
    <TextField fx:id="phoneField" maxWidth="150">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin></TextField>
    <Button fx:id="button" mnemonicParsing="false" onAction="#handleButtonAction" text="Add">
         <VBox.margin>
            <Insets bottom="20.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin></Button>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="75.0" text="id" />
          <TableColumn fx:id="nameCol" prefWidth="75.0" text="name" />
          <TableColumn fx:id="phoneCol" prefWidth="75.0" text="phone" />
          <TableColumn fx:id="cityCol" prefWidth="75.0" text="city" />
        </columns>
      </TableView>
   </children>
</VBox>
